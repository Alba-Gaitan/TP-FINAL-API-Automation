{
  "url": "https://api.clockify.me/api/v1",
  "headers": {
    "X-Api-Key": "{{api-key}}",
    "Content-Type": "application/json"
  },

  "body": {
    "name": "{{name}}",
    "isPublic": "false",
    "hourlyRate": {
      "amount": 3600
    },
    "color": "#f44336",
    "billable": "true",
    "archived": "true"
  },

  "endpoint": "/workspaces/{{wSpace}}/projects/{{id_project}}"
}